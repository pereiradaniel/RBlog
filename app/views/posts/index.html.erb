<h1>#PostsIndex</h1>
<div>
	<%= link_to 'New Post', new_post_path %>
</div>
<div>
	<% @posts.each do |post| %>
		<div>
			<div>
				<%= post.text %>
			</div>
			<div>
				<div>
					<%= link_to 'View', post_path(post) %>
				</div>
				<div>
					<%= link_to 'Edit', edit_post_path(post) %>
				</div>
			</div>
		</div>
	<% end %>
</div>